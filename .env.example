# Qdrant configuration
QDRANT_URL=http://localhost:6333
# QDRANT_API_KEY=  # Leave empty for local installations, required for cloud deployments

# OpenAI configuration (required for default embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# Agent configuration
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSION=384  # Adjust this based on your embedding model
VERBOSE=True  # Set to False for less output

# Advanced settings
BATCH_SIZE=100  # Number of documents to process in a batch
